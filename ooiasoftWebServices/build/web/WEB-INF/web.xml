<?xml version="1.0" encoding="UTF-8"?>
<web-app version="4.0" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd">
    <servlet>
        <servlet-name>ReporteEventos</servlet-name>
        <servlet-class>pe.edu.pucp.ooiasoft.servlets.ReporteEventos</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ReporteCitas</servlet-name>
        <servlet-class>pe.edu.pucp.ooiasoft.servlets.ReporteCitas</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ReporteEventos</servlet-name>
        <url-pattern>/ReporteEventos</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ReporteCitas</servlet-name>
        <url-pattern>/ReporteCitas</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
